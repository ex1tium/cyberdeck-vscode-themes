{
  "name": "cyberdeck-2025",
  "displayName": "Cyberdeck 2025",
  "description": "A retro-futuristic neural interface for the modern code cowboy. Cyberpunk-inspired dark theme with neon accents, semantic highlighting, and first-class support for TypeScript, Go, Rust, Python, and Java.",
  "version": "1.1.2",
  "publisher": "ex1tium",
  "author": {
    "name": "ex1tium"
  },
  "license": "MIT",
  "icon": "icon.png",
  "repository": {
    "type": "git",
    "url": "https://github.com/ex1tium/cyberdeck-2025_vscode_theme"
  },
  "homepage": "https://github.com/ex1tium/cyberdeck-2025_vscode_theme#readme",
  "bugs": {
    "url": "https://github.com/ex1tium/cyberdeck-2025_vscode_theme/issues"
  },
  "keywords": [
    "cyberdeck",
    "theme",
    "dark theme",
    "cyberpunk",
    "neon",
    "retro",
    "futuristic",
    "synthwave",
    "color theme",
    "semantic highlighting",
    "typescript",
    "rust",
    "go",
    "python",
    "java",
    "zig"
  ],
  "galleryBanner": {
    "color": "#130d1a",
    "theme": "dark"
  },
  "engines": {
    "vscode": "^1.104.0"
  },
  "categories": [
    "Themes"
  ],
  "contributes": {
    "themes": [
      {
        "label": "Cyberdeck 2025",
        "uiTheme": "vs-dark",
        "path": "./themes/Cyberdeck-2025-color-theme.json"
      },
      {
        "label": "Cyberdeck 2025 Midnight",
        "uiTheme": "vs-dark",
        "path": "./themes/Cyberdeck-2025-Midnight-color-theme.json"
      }
    ]
  },
  "scripts": {
    "build": "node src/build.js",
    "watch": "node src/build.js && echo 'Watching for changes...' && nodemon --watch src -e js --exec 'node src/build.js'"
  },
  "devDependencies": {
    "prettier": "^3.6.2"
  }
}
